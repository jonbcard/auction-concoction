.block
  .secondary-navigation
    %ul.wat-cf
      %li.first=link_to pat(:list), url(:bidders, :index, :auctions_id => @auction.id)
      %li.active=link_to pat(:new), url(:bidders, :new, :auctions_id => @auction.id)
  .content
    %h2.title New Bidder
    .inner
      -form_for :bidder, nil, :class => :form do |f|
        =partial "bidders/form", :locals => { :f => f }